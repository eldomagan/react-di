{
  "name": "react-di",
  "version": "0.2.0",
  "description": "Dependency management for React.js",
  "main": "dist/react-di.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/janjakubnanista/react-di"
  },
  "bugs": {
    "url": "https://github.com/janjakubnanista/react-di/issues"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "./node_modules/karma/bin/karma start test/karma.conf.js",
    "dev": "./node_modules/karma/bin/karma start test/karma.conf.js --auto-watch --no-single-run",
    "build": "rm -rf dist && npm test && gulp build && gulp build --minify"
  },
  "keywords": [
    "react",
    "react.js",
    "di",
    "dependency injection"
  ],
  "author": {
    "name": "Ján Jakub Naništa",
    "email": "jan.jakub.nanista@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "envify": "^1.2.1",
    "expect.js": "^0.3.1",
    "gulp": "^3.8.10",
    "gulp-concat": "^2.4.3",
    "gulp-if": "^1.2.5",
    "gulp-uglify": "^1.0.2",
    "gulp-webpack": "^1.1.2",
    "karma": "^0.12.31",
    "karma-browserify": "^2.0.0",
    "karma-coverage": "^0.2.7",
    "karma-firefox-launcher": "^0.1.4",
    "karma-mocha": "^0.1.10",
    "karma-sinon-expect": "^0.1.4",
    "karma-spec-reporter": "0.0.16",
    "mocha": "^2.1.0",
    "react": "^0.12.2",
    "webpack": "^1.4.15",
    "yargs": "^1.3.3"
  }
}
