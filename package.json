{
  "name": "react-di",
  "version": "0.0.4",
  "description": "Dependency management for React.js",
  "main": "dist/mixin.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha test/*_test.js",
    "dev": "mocha --watch lib/*.js test/*_test.js",
    "build": "rm -rf dist && webpack lib/mixin.js dist/react-di.js --output-library ReactDI --output-library-target umd"
  },
  "keywords": [
    "react",
    "react.js",
    "di",
    "dependency injection"
  ],
  "author": {
    "name": "Ján Jakub Naništa",
    "email": "jan.jakub.nanista@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "expect.js": "^0.3.1",
    "mocha": "^2.1.0",
    "webpack": "^1.4.15"
  }
}
